              <input id="id_email" maxlength="250" name="email" class="required email" type="email" placeholder="E-mail*">
              <textarea id="id_comments" name="comments" required="True" placeholder="Questions/Comments"></textarea>
<!--               <input type="submit" value="Send" class="btn btn-primary" style="display: inline-block"> -->
              <%= link_to "Send", thankyou_path, class: "btn btn-primary", style: "display: inline-block" %>
            </form>
          </div>          
        </div>
        <ul class="footer-links">
          <li><a href="../privacy/">Privacy Policy</a></li>
