            <p>Phone: 402-123-4567</p>
            <p>Fax: 402-234-5678</p>
            <p style="margin-bottom: 20px;">Email: <a href="mailto:contact@radialworks.com">contact@radialworks.com</a></p>
            <%= link_to "CONTACT US", contact_path, class: "btn btn-inverse btn-large", style: "margin-bottom: 25px;" %>
